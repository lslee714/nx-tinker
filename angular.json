{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "app-with-nx": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/app-with-nx",
      "sourceRoot": "apps/app-with-nx/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/app-with-nx",
            "index": "apps/app-with-nx/src/index.html",
            "main": "apps/app-with-nx/src/main.ts",
            "polyfills": "apps/app-with-nx/src/polyfills.ts",
            "tsConfig": "apps/app-with-nx/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/app-with-nx/src/favicon.ico",
              "apps/app-with-nx/src/assets"
            ],
            "styles": ["apps/app-with-nx/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app-with-nx/src/environments/environment.ts",
                  "with": "apps/app-with-nx/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-with-nx:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-with-nx:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-with-nx:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/app-with-nx/src/test.ts",
            "polyfills": "apps/app-with-nx/src/polyfills.ts",
            "tsConfig": "apps/app-with-nx/tsconfig.spec.json",
            "karmaConfig": "apps/app-with-nx/karma.conf.js",
            "assets": [
              "apps/app-with-nx/src/favicon.ico",
              "apps/app-with-nx/src/assets"
            ],
            "styles": ["apps/app-with-nx/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/app-with-nx/tsconfig.app.json",
              "apps/app-with-nx/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "app-with-nx-e2e": {
      "projectType": "application",
      "root": "apps/app-with-nx-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/app-with-nx-e2e/protractor.conf.js",
            "devServerTarget": "app-with-nx:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-with-nx:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/app-with-nx-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "test": {
      "projectType": "library",
      "root": "libs/test",
      "sourceRoot": "libs/test/src",
      "prefix": "app-with-nx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/test/tsconfig.lib.json",
              "libs/test/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/test/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/test/jest.config.js",
            "tsConfig": "libs/test/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/test/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "hello": {
      "projectType": "library",
      "root": "libs/hello",
      "sourceRoot": "libs/hello/src",
      "prefix": "app-with-nx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/hello/tsconfig.lib.json",
              "libs/hello/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/hello/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/hello/jest.config.js",
            "tsConfig": "libs/hello/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/hello/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "world": {
      "projectType": "library",
      "root": "libs/world",
      "sourceRoot": "libs/world/src",
      "prefix": "app-with-nx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/world/tsconfig.lib.json",
              "libs/world/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/world/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/world/jest.config.js",
            "tsConfig": "libs/world/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/world/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "unrelated": {
      "projectType": "library",
      "root": "libs/unrelated",
      "sourceRoot": "libs/unrelated/src",
      "prefix": "app-with-nx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/unrelated/tsconfig.lib.json",
              "libs/unrelated/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/unrelated/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/unrelated/jest.config.js",
            "tsConfig": "libs/unrelated/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/unrelated/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "another-app": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/another-app",
      "sourceRoot": "apps/another-app/src",
      "prefix": "app-with-nx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/another-app",
            "index": "apps/another-app/src/index.html",
            "main": "apps/another-app/src/main.ts",
            "polyfills": "apps/another-app/src/polyfills.ts",
            "tsConfig": "apps/another-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/another-app/src/favicon.ico",
              "apps/another-app/src/assets"
            ],
            "styles": ["apps/another-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/another-app/src/environments/environment.ts",
                  "with": "apps/another-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "another-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "another-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "another-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/another-app/tsconfig.app.json",
              "apps/another-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/another-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/another-app/jest.config.js",
            "tsConfig": "apps/another-app/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/another-app/src/test-setup.ts"
          }
        }
      }
    },
    "another-app-e2e": {
      "root": "apps/another-app-e2e",
      "sourceRoot": "apps/another-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/another-app-e2e/cypress.json",
            "tsConfig": "apps/another-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "another-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "another-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/another-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/another-app-e2e/**"]
          }
        }
      }
    }
  },
  "defaultProject": "app-with-nx",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
